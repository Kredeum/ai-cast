		"type-fest": "^4.23.0",
		"typescript": "^5.5.4",
		"vercel": "^35.2.3",
		"vite": "^5.3.5",
		"vitest": "^2.0.5"
	},
	"type": "module",
	"dependencies": {
		"@byteatatime/wagmi-svelte": "^0.1.1",
